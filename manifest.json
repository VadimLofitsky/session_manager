{
  "manifest_version": 3,
  "name": "Session manager",
  "description": "Сохранение и восстановление вкладок",
  "version": "1.0",

  "permissions": [
    "tabs",
    "storage"
  ],

  "action": {
    "default_popup": "popup.html"
  },

  "web_accessible_resources": [
    {
      "resources": [
        "js/*",
        "css/styles.css",
        "bootstrap/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
